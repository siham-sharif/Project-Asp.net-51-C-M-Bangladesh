@using System.Web.UI.WebControls
@using WebM.Models



@model IEnumerable<WebM.Models.MedicineStock>
@{
    ViewBag.Title = "MedicineStockReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Medicine Stock Report</h2>


<table class="table" id="myTable">
    <thead>
        <tr>
            <th>
                @Html.Label("Medicine Name", new { @class = "control-label col-md-6" })
            </th>
            <th>
                @Html.Label("Present Stock", new { @class = "control-label col-md-6" })
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MedicineName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MedicineQuantity)
                </td>    
            </tr>
        }
    </tbody>

</table>